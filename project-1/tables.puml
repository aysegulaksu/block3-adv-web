@startuml

abstract class PetProduct {
  - name
  - type
  - category
  - price
  - tax
  -brand
  -cost
  -profitMargin

  + __construct()
  + getName()
  + getType()
  + getCategory()
  + getPrice()
  + getTax()
  +getBrand
  +getCost
  abstract calculateTax()
  abstract calculateSalePrice()
}

class PetFood extends PetProduct {
  - expiration

  + __construct()
  + getExpiration()
  + calculateExpiration()
  + calculateTax()
  + calculateSalePrice()
}

class PetToy extends PetProduct {

  + __construct()
  + calculateTax()
  + calculateSalePrice()
}

@enduml

